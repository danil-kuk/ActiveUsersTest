{
    "extends": "standard",
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "no-debugger": 0,
      "no-console": 0,
      "new-cap": 0,
      "strict": 0,
      "no-underscore-dangle": 0,
      "no-use-before-define": 0,
      "eol-last": 0,
      "quotes": ["error", "single", { "allowTemplateLiterals": true }],
      "semi": [2, "never"],
      "space-before-function-paren": ["warn", "never"],
      "eqeqeq": 0
    }
  }